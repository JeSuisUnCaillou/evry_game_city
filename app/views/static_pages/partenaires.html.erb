
<div class="panel transparent">
  <div class="panel-body text-center bold">
    <h1><%= t(:partenaires) %></h1>
  </div>
</div>


  <div class="row partenaires">
   <% (1..4).each do |i| %>
      <%= render partial: 'static_pages/partenaires/partenaire_mosaique', locals: { i:i} %>
   <% end  %>
  </div>


<div class="jcarousel" data-jcarousel="true">
  <ul>
    <% (1..8).each do |i| %>
      <li>
       <%= render partial: 'static_pages/partenaires/partenaire_mosaique', locals: { i:i} %>
        <%#= image_tag t("partenaire-#{i}.image"), class: "center-block", alt: t("partenaire-#{i}.name") %>
      </li>
    <% end  %>

  </ul>
</div>



